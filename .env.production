# Production Environment Variables for Vercel
# Add these to your Vercel dashboard

# Clerk Authentication (Required)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Application URL (Set automatically by Vercel)
NEXT_PUBLIC_APP_URL=

# AI APIs (Optional)
NEXT_PUBLIC_OPEN_ROUTER_API=
NEXT_PUBLIC_GPTGOD_API=
NEXT_PUBLIC_HF_TOKEN=
REPLICATE_API_TOKEN=

# Chat UI Integration
NEXT_PUBLIC_CHATUI_URL=https://your-chatui.vercel.app
